{
   "name": "candidate-hub-ui",
   "author": "Broadbean",
   "version": "1.0.0",
   "description": "Boilerplate node application",
   "main": "lib/app.js",
   "scripts": {
      "build": "gulp scripts",
      "test": "mocha",
      "dev": "NODE_ENV=development nodemon src/app.js",
      "start": "pm2 start process.json --env production",
      "tidy": "prettier --write '{lib,test}/**/*.js'",
      "test:watch": "nodemon -x nyc --reporter=lcovonly mocha -R spec"
   },
   "nodemonConfig": {
      "ignore": [
         "node_modules/**/node_modules"
      ],
      "delay": "2500",
      "env": {
         "NODE_ENV": "development",
         "NODE_CONFIG_DIR": "./config"
      }
   },
   "license": "UNLICENSED",
   "private": true,
   "devDependencies": {
      "chai": "^4.1.2",
      "chai-http": "^4.3.0",
      "gulp": "^4.0.0",
      "gulp-sass": "^4.0.1",
      "mocha": "^5.2.0",
      "mocha-eslint": "^4.1.0",
      "nodemon": "^1.18.3",
      "pm2": "^2.10.1",
      "prettier": "^1.11.1",
      "sinon": "^6.1.5"
   },
   "dependencies": {
      "app-root-path": "^2.2.1",
      "bluebird": "^3.5.1",
      "bootstrap": "^4.1.1",
      "config": "^1.31.0",
      "cors": "^2.8.5",
      "debug": "^3.1.0",
      "ejs": "^2.6.1",
      "express": "^4.16.2",
      "gulp": "^4.0.0",
      "hbs": "^4.0.1",
      "helmet": "^3.13.0",
      "jquery": "^3.3.1",
      "jsonfile": "^4.0.0",
      "knex": "^0.20.1",
      "knex-tiny-logger": "^1.1.0",
      "knexnest": "^1.0.0",
      "morgan": "^1.9.0",
      "mysql2": "^1.5.1",
      "node-cache": "^4.2.0",
      "noty": "^3.2.0-beta",
      "popper.js": "^1.14.3",
      "winston": "^3.2.1"
   }
}
